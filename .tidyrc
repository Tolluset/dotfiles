# .tidyrc
# config file of HTML Tidy
#
# author janus_wel <janus.wel.3@gmail.com>
# Last Change: 2009/02/01 20:03:56.

bare:yes
char-encoding:raw
clean:yes
fix-backslash:yes
fix-bad-comments:yes
indent-attributes:no
indent-cdata:no
indent-spaces:2
indent:auto
newline:LF
replace-color:yes
wrap-asp:no
wrap-attributes:no
wrap-jste:no
wrap-php:no
wrap-script-literals:no
wrap-sections:no
wrap:0
